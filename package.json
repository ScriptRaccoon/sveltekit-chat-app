{
	"name": "sveltekit-chat-app-2",
	"version": "0.0.1",
	"private": true,
	"main": "server.js",
	"scripts": {
		"start": "node server.js",
		"dev": "vite dev",
		"build": "vite build && tsc server.ts --esModuleInterop --target esnext --moduleResolution node",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^1.0.0",
		"@sveltejs/adapter-node": "^1.1.4",
		"@sveltejs/kit": "^1.0.0",
		"@types/express": "^4.17.16",
		"@types/node": "^18.11.18",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^4.0.0"
	},
	"type": "module",
	"dependencies": {
		"express": "^4.18.2",
		"socket.io": "^4.5.4",
		"socket.io-client": "^4.5.4"
	}
}